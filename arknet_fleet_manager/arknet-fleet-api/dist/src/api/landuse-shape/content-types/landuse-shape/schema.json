{
    "kind": "collectionType",
    "collectionName": "landuse_shapes",
    "info": {
        "singularName": "landuse-shape",
        "pluralName": "landuse-shapes",
        "displayName": "Landuse Shape",
        "description": "Stores coordinate points for landuse zone geometries (Polygons, MultiPolygons)"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "shape_pt_lat": {
            "type": "float",
            "required": true
        },
        "shape_pt_lon": {
            "type": "float",
            "required": true
        },
        "shape_pt_sequence": {
            "type": "integer",
            "required": true
        },
        "ring_index": {
            "type": "integer",
            "default": 0
        },
        "polygon_index": {
            "type": "integer",
            "default": 0
        },
        "landuse_zone": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::landuse-zone.landuse-zone",
            "inversedBy": "landuse_shapes"
        }
    }
}
