{
    "kind": "collectionType",
    "collectionName": "region_shapes",
    "info": {
        "singularName": "region-shape",
        "pluralName": "region-shapes",
        "displayName": "Region Shape",
        "description": "Stores coordinate points for region geometries (Polygons, MultiPolygons)"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "shape_pt_lat": {
            "type": "float",
            "required": true
        },
        "shape_pt_lon": {
            "type": "float",
            "required": true
        },
        "shape_pt_sequence": {
            "type": "integer",
            "required": true
        },
        "ring_index": {
            "type": "integer",
            "default": 0
        },
        "polygon_index": {
            "type": "integer",
            "default": 0
        },
        "region": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::region.region",
            "inversedBy": "region_shapes"
        }
    }
}
