{
  "kind": "collectionType",
  "collectionName": "shapes",
  "info": {
    "singularName": "shape",
    "pluralName": "shapes",
    "displayName": "Shape",
    "description": "Route geometry shapes"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "shape_id": {
      "type": "string",
      "required": true
    },
    "shape_pt_lat": {
      "type": "float",
      "required": true
    },
    "shape_pt_lon": {
      "type": "float",
      "required": true
    },
    "shape_pt_sequence": {
      "type": "integer",
      "required": true
    },
    "shape_dist_traveled": {
      "type": "decimal",
      "required": false
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "trips": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::trip.trip",
      "mappedBy": "shape"
    }
  }
}