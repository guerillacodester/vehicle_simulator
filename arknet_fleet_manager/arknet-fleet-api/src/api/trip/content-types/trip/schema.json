{
  "kind": "collectionType",
  "collectionName": "trips",
  "info": {
    "singularName": "trip",
    "pluralName": "trips",
    "displayName": "Trip",
    "description": "Individual trips on routes"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "trip_id": {
      "type": "uid",
      "required": true,
      "targetField": "trip_headsign"
    },
    "trip_headsign": {
      "type": "string",
      "required": false
    },
    "trip_short_name": {
      "type": "string",
      "required": false
    },
    "direction_id": {
      "type": "enumeration",
      "enum": ["outbound", "inbound"],
      "required": false
    },
    "block_id": {
      "type": "string",
      "required": false
    },
    "wheelchair_accessible": {
      "type": "enumeration",
      "enum": ["unknown", "accessible", "not_accessible"],
      "default": "unknown",
      "required": false
    },
    "bikes_allowed": {
      "type": "enumeration",
      "enum": ["unknown", "allowed", "not_allowed"],
      "default": "unknown",
      "required": false
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "route": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::route.route",
      "inversedBy": "trips"
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "trips"
    },
    "shape": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shape.shape",
      "inversedBy": "trips"
    }
  }
}