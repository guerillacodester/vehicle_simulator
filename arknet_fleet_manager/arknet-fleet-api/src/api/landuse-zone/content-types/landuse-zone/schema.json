{
  "kind": "collectionType",
  "collectionName": "landuse_zones",
  "info": {
    "singularName": "landuse-zone",
    "pluralName": "landuse-zones",
    "displayName": "Land Use Zone",
    "description": "Land use zones for understanding population density and spawning patterns"
  },
  "options": {
    "draftAndPublish": false,
    "comment": "Geographic zones with different land use types (residential, commercial, etc.)"
  },
  "pluginOptions": {},
  "attributes": {
    "zone_type": {
      "type": "enumeration",
      "enum": [
        "residential",
        "commercial",
        "industrial",
        "farmland",
        "forest",
        "recreation",
        "institutional",
        "mixed_use",
        "transportation",
        "water",
        "other"
      ],
      "required": true,
      "default": "other"
    },
    "name": {
      "type": "string",
      "maxLength": 255
    },
    "description": {
      "type": "text"
    },
    "geometry_geojson": {
      "type": "json",
      "required": true
    },
    "center_latitude": {
      "type": "decimal",
      "min": -90,
      "max": 90
    },
    "center_longitude": {
      "type": "decimal",
      "min": -180,
      "max": 180
    },
    "area_sq_km": {
      "type": "decimal",
      "min": 0
    },
    "population_density": {
      "type": "decimal",
      "default": 0,
      "min": 0
    },
    "spawn_weight": {
      "type": "decimal",
      "default": 1.0,
      "min": 0,
      "max": 10
    },
    "peak_hour_multiplier": {
      "type": "decimal",
      "default": 1.0,
      "min": 0,
      "max": 5
    },
    "off_peak_multiplier": {
      "type": "decimal",
      "default": 1.0,
      "min": 0,
      "max": 5
    },
    "tags": {
      "type": "json",
      "default": []
    },
    "osm_id": {
      "type": "string",
      "unique": false
    },
    "is_active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "landuse_zones"
    },
    "region": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::region.region",
      "inversedBy": "landuse_zones"
    },
    "landuse_shapes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::landuse-shape.landuse-shape",
      "mappedBy": "landuse_zone"
    }
  }
}
