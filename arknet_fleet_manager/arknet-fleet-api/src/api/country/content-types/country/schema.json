{
  "kind": "collectionType",
  "collectionName": "countries",
  "info": {
    "singularName": "country",
    "pluralName": "countries",
    "displayName": "Country",
    "description": "Countries in the fleet management system with geographic data import"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "country_id": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "code": {
      "type": "string",
      "maxLength": 3,
      "required": true,
      "unique": true
    },
    "currency": {
      "type": "string",
      "maxLength": 3,
      "required": false
    },
    "timezone": {
      "type": "string",
      "required": false
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "routes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::route.route",
      "mappedBy": "country"
    },
    "vehicles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vehicle.vehicle",
      "mappedBy": "country"
    },
    "drivers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::driver.driver",
      "mappedBy": "country"
    },
    "depots": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::depot.depot",
      "mappedBy": "country"
    },
    "pois": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::poi.poi",
      "mappedBy": "country"
    },
    "landuse_zones": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::landuse-zone.landuse-zone",
      "mappedBy": "country"
    },
    "regions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::region.region",
      "mappedBy": "country"
    },
    "places": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::place.place",
      "mappedBy": "country"
    },
    "highways": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::highway.highway",
      "mappedBy": "country"
    },
    "spawn_config": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::spawn-config.spawn-config",
      "mappedBy": "country"
    },
    "geodata_import_status": {
      "type": "json",
      "default": {
        "highway": {
          "status": "not_imported",
          "lastImportDate": null,
          "featureCount": 0,
          "lastJobId": null
        },
        "amenity": {
          "status": "not_imported",
          "lastImportDate": null,
          "featureCount": 0,
          "lastJobId": null
        },
        "landuse": {
          "status": "not_imported",
          "lastImportDate": null,
          "featureCount": 0,
          "lastJobId": null
        },
        "building": {
          "status": "not_imported",
          "lastImportDate": null,
          "featureCount": 0,
          "lastJobId": null
        },
        "admin": {
          "status": "not_imported",
          "lastImportDate": null,
          "featureCount": 0,
          "lastJobId": null
        }
      }
    },
    "geodata_last_import": {
      "type": "datetime"
    },
    "import_highway": {
      "type": "customField",
      "customField": "plugin::action-buttons.button-field",
      "options": {
        "buttonLabel": "üõ£Ô∏è Import Highways",
        "onClick": "handleImportHighway"
      }
    }
  }
}
