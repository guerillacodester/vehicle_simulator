{
  "kind": "collectionType",
  "collectionName": "feed_info",
  "info": {
    "singularName": "feed-info",
    "pluralName": "feed-infos",
    "displayName": "Feed Info",
    "description": "GTFS feed metadata and publisher information (GTFS feed_info.txt)"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "feed_publisher_name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "feed_publisher_url": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "feed_lang": {
      "type": "string",
      "required": true,
      "default": "en",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "default_lang": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "feed_start_date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "feed_end_date": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "feed_version": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "feed_contact_email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "feed_contact_url": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "is_active": {
      "type": "boolean",
      "default": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
